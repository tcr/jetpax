     47  fada				   .VLB       EQU	{2}
     48  fada				   .RES       EQU	{3}
     49  fada					      CLC
     50  fada					      LDA	.VLA+3
     51  fada					      ADC	.VLB+3
     52  fada					      STA	.RES+3
     53  fada					      LDA	.VLA+2
     54  fada					      ADC	.VLB+2
     55  fada					      STA	.RES+2
     56  fada					      LDA	.VLA+1
     57  fada					      ADC	.VLB+1
     58  fada					      STA	.RES+1
     59  fada					      LDA	.VLA+0
     60  fada					      ADC	.VLB+0
     61  fada					      STA	.RES+0
     62  fada					      ENDM
